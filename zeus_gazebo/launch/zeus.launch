<?xml version="1.0" ?>

<launch>

  <include file="$(find mars)/launch/mars_env_only.launch">
  </include>

  <!-- Convert an xacro and put on parameter server -->
  <param name="robot_description" command="$(find xacro)/xacro.py $(find zeus_description)/urdf/zeus.urdf.xacro" />

  <!-- Spawn a robot into Gazebo -->
  <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-param robot_description -urdf -model zeus" />
</launch>
