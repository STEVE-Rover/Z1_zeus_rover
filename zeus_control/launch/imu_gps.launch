<?xml version="1.0" ?>

<launch>

  <!-- Launch imu filter -->
  <node pkg="imu_filter_madgwick" type="imu_filter_node" name="imu_filter_node" output="screen">
   <param name="_use_mag" value="false" />
  </node>

  <!-- Launch imu -->
  <node pkg="mpu_6050_driver" type="imu_node.py" name="imu_node" output="screen">
  </node>

  
  <!-- Launch gps -->
  <!-- Do not forget to give access to the port : sudo chmod 666 /dev/ttyTHS1 -->
  <node pkg="nmea_navsat_driver" type="nmea_serial_driver" name="imu_node" output="screen">
  <param name="_port" value="/dev/ttyTHS1" />
  <param name="_baud" value="115200" />
  </node>
  
</launch>
