<?xml version="1.0"?>
<robot name="myfirst">

  <material name="gray">
    <color rgba="0.5 0.5 0.5 1"/>
  </material>

  <link name="base_link">
    <visual>
      <geometry>
        <mesh filename="file:///home/samuel/zeus/catkin_ws/src/zeus_description/Z1.stl"/>
      </geometry>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <material name="gray"/>
    </visual>
    <collision>
      <geometry>
        <box size="1.3 0.75 1.3"/>
      </geometry>
      <origin rpy="0 0 0" xyz="-0.1 0 0"/>
    </collision>
    <inertial>
      <mass value="50"/>
      <inertia ixx="0.4" ixy="0.0" ixz="0.0" iyy="0.4" iyz="0.0" izz="0.4"/>
    </inertial>
  </link>

  <link name="left_front_wheel">
    <visual>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.1" radius="0.1"/>
      </geometry>
      <material name="black"/>
    </visual>
  </link>

  <link name="right_front_wheel">
    <visual>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.1" radius="0.1"/>
      </geometry>
      <material name="black"/>
    </visual>
  </link>

  <link name="left_rear_wheel">
    <visual>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.1" radius="0.1"/>
      </geometry>
      <material name="black"/>
    </visual>
  </link>

  <link name="right_rear_wheel">
    <visual>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.1" radius="0.1"/>
      </geometry>
      <material name="black"/>
    </visual>
  </link>









  <joint name="left_front_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="left_front_wheel"/>
    <origin rpy="0 0 0" xyz="0 0.3 -0.6"/>
    <axis xyz="0 1 0"/>
  </joint>

  <joint name="right_front_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="right_front_wheel"/>
    <origin rpy="0 0 0" xyz="0 -0.3 -0.6"/>
    <axis xyz="0 1 0"/>
  </joint>

  <joint name="left_rear_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="left_rear_wheel"/>
    <origin rpy="0 0 0" xyz="-0.6 0.3 -0.6"/>
    <axis xyz="0 1 0"/>
  </joint>

  <joint name="right_rear_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="right_rear_wheel"/>
    <origin rpy="0 0 0" xyz="-0.6 -0.3 -0.6"/>
    <axis xyz="0 1 0"/>
  </joint>

</robot>