  <joint name="rocker_right_joint" type="fixed">
    <parent link="base_link"/>
    <child link="rocker_right"/>
    <origin rpy="0 -0.2 0" xyz="-0.3 -0.3 -0.4"/>
    <!-- <axis xyz="0 1 0"/>
    <dynamics damping="0.0" friction="10.0"/> -->
  </joint>

  <joint name="bogie_right_joint" type="continuous">
    <parent link="rocker_right"/>
    <child link="bogie_right"/>
    <origin rpy="0 -0.5 0" xyz="0.35 0 -0.2"/>
    <axis xyz="0 1 0"/>
    <dynamics damping="0.0" friction="10.0"/>
  </joint>

  <joint name="rocker_left_joint" type="fixed">
    <parent link="base_link"/>
    <child link="rocker_left"/>
    <origin rpy="0 -0.2 0" xyz="-0.3 0.3 -0.4"/>
    <!-- <axis xyz="0 1 0"/>
    <dynamics damping="0.0" friction="10.0"/>
    <mimic joint="rocker_right_joint" multiplier="-1"/> -->
  </joint>

